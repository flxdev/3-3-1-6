					script(type='text/javascript').
						$(heightScrollContainer)
						$(scrollbar)
						$(iso)
						$(lightG)
			a(class="back ajax-trigger" href="#")
				div
					<svg viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><path d="m8.46203,12.45218l3.03554,3.03553l-0.70711,0.70711l-4.24264,-4.24264l4.24264,-4.24264l0.70711,0.70711l-3.0371,3.03709l9.08263,0l0,0.99844l-9.08107,0zm15.08343,-0.49764c0,-6.35127 -5.14873,-11.5 -11.5,-11.5c-6.35127,0 -11.5,5.14873 -11.5,11.5c0,6.35127 5.14873,11.5 11.5,11.5c6.35127,0 11.5,-5.14873 11.5,-11.5z"/></svg>
